{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "indent": ["error", 4],
        "one-var": 0,
        "no-multi-spaces": [1, {
            "exceptions": {
                "VariableDeclarator": true
            }
        }],
        "no-console": 0,
        "react/prop-types": 0,
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4]
    }
}
